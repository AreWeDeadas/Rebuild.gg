            if typeof(bs) == "function" then
                table.insert(self._hit_listeners, bs)
            end
        end
        function C:OnTargetUpdate(bs)
            if typeof(bs) == "function" then
                table.insert(self._target.listeners, bs)
            end
        end
        function C:_emit_hit(aD)
            for bc, bt in ipairs(self._hit_listeners) do
