                                bg.CanCollide = bg:GetAttribute("OriginalCollision")
                                bg.CanTouch = bg:GetAttribute("OriginalTouch")
                                bg.CanQuery = bg:GetAttribute("OriginalQuery")
                                bg:SetAttribute("OriginalCollision", nil)
                                bg:SetAttribute("OriginalTouch", nil)
