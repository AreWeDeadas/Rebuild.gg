                    local aa = jn or {}
                    if #jp > 1 and not jn then
                        for av = 2, #jp do
                            table.insert(aa, jp[av])
                        end
                    end
                    local gV = jh.map[jr]
                    if gV then
                        return gV(aa, iX)
                    end
                elseif type(jm) == "table" and jm.name then
                    local gV = jh.map[string.lower(jm.name)]
                    if gV then
                        return gV(jm.args or {}, iX)
                    end
                end
            end
