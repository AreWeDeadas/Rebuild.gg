            local function jZ(eq, state)
                if not eq then
                    return
                end
                for bc, bg in ipairs(eq:GetDescendants()) do
                    if bg:IsA("BasePart") then
                        if state then
                            if bg:GetAttribute("OriginalCollision") == nil then
