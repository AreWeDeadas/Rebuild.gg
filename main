                            pcall(
                                function()
                                    fy:Stop()
                                    fy.Volume = 0
                                    fy.Playing = false
                                end
                            )
                            if not fF.Sound._guarded then
                                fF.Sound._guarded = {}
                            end
                            if not fF.Sound._guarded[fy] then
                                fF.Sound._guarded[fy] =
                                    fy:GetPropertyChangedSignal("Playing"):Connect(
                                    function()
                                        if g and g["hs_mute_original"] and fy.Playing then
                                            pcall(
                                                function()
                                                    fy:Stop()
                                                    fy.Volume = 0
                                                end
                                            )
                                        end
                                    end
                                )
                            end
                            return true
                        end
                    end
                end
                return false
            end
