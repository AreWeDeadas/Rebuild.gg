        local desync_utility = {}
        function desync_utility:ValidateClient(az)
            if not az then
                return false
            end
            local a3 = az.Character
            if not a3 then
                return false
            end
            local aA = a3:FindFirstChildOfClass("Humanoid")
            if not aA or aA.Health <= 0 then
