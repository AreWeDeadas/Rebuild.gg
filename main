                    bw.AssemblyLinearVelocity =
                        jE * (fK.WalkSpeed + dU) + B.Vector3.new(0, bw.AssemblyLinearVelocity.Y, 0)
                end}
            local function jH(cp, dU, bw, fK)
                local gR = jA.correction.mode
                if gR == "None" then
                    return cp
                end
                if cp.Magnitude > 1e-3 then
                    cp = cp.Unit
                else
                    return cp
                end
